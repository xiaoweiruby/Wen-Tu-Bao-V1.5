{
  "manifest_version": 3,
  "name": "文图宝",
  "version": "1.5",
  "description": "您的文字转图片助手",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "downloads"
  ]
}
